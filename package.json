{
  "name": "hnpwa-vanilla",
  "version": "1.5.0",
  "description": "HN PWA build with no framework",
  "main": "index.js",
  "scripts": {
    "start-prod": "npm run build-prod; NODE_ENV=production PORT=80 node server.js",
    "serve": "webpack-dev-server --progress --inline --port 8080 --config webpack.config.js",
    "build-sw": "node sw-build.js",
    "html-minify": "node html-minify.js",
    "build-prod": "rm -rf public; NODE_ENV=production webpack --config webpack.config.js; npm run build-sw; npm run html-minify",
    "deploy": "npm run build-prod; firebase deploy"
  },
  "keywords": [],
  "author": "Cristian Bote",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "bundle-loader": "^0.5.5",
    "css-loader": "^0.26.4",
    "express": "^4.15.3",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-dev-server": "^1.16.5"
  },
  "devDependencies": {
    "babel-preset-es2015-native-modules": "^6.9.4",
    "copy-webpack-plugin": "^4.0.1",
    "file-loader": "^0.9.0",
    "html-minifier": "^3.5.2",
    "node-sass": "^4.3.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
