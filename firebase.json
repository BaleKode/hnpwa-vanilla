{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
      "source": "**",
      "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "http://*",
        "destination": "https://*",
        "type": 301
      }
    ],
    "headers": [{
      "source" : "**/*.@(jpg|jpeg|gif|png)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=7200"
      }]
    }]
  }
}
