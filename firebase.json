{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
      "source": "**",
      "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "http://hnpwa-vanilla.firebaseapp.com/",
        "destination":"https://hnpwa-vanilla.firebaseapp.com/",
        "type": 301
      }
    ],
    "headers": [
      {
        "source" : "**/*.@(jpg|jpeg|gif|png|ico)",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "max-age=7200"
        }]
      },
      {
        "source" : "sw.js",
        "headers" : [
          {
            "key" : "Cache-Control",
            "value" : "max-age=0, no-cache, no-store, must-revalidate"
          }
        ]
      }
    ]
  }
}
